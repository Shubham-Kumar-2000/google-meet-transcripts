{
  "name": "Google Meet Transcripts",
  "description": "Record Google Meet Transcripts",
  "version": "1",
  "content_scripts": [{
    "matches": ["https://meet.google.com/*"],
    "js": ["lib/inject.js"],
    "run_at": "document_idle",
    "all_frames": false
  }],
  "web_accessible_resources": ["lib/stenographer.js"],
  "manifest_version": 2
}
